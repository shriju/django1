# Ignore virtual environments
myenv/
venv/

# Ignore Django cache and compiled files
*.pyc
__pycache__/

# Ignore Django migrations
migrations/*.pyc
migrations/*.py
migrations/__pycache__/

# Ignore SQLite database (if used for local development)
db.sqlite3
*.db

# Ignore Django static and media files
static/
media/

# Ignore environment files with secrets
*.env
*.env.*
config.json

# Ignore IDE-specific files (like Visual Studio Code, PyCharm, etc.)
*.vscode/
*.idea/

# Ignore other build artifacts
build/
dist/
*.egg-info/

# Ignore Node.js modules (if applicable)
node_modules/

# Ignore SQL dumps or backups
*.sql  
